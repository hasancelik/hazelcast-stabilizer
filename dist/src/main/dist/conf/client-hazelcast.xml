<?xml version="1.0" encoding="UTF-8"?>

<hazelcast-client
        xsi:schemaLocation="http://www.hazelcast.com/schema/client-config
            http://www.hazelcast.com/schema/config/hazelcast-client-config-3.2.xsd"
        xmlns="http://www.hazelcast.com/schema/client-config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>workers</name>
    </group>

    <network>
        <cluster-members>
            <!--MEMBERS-->
        </cluster-members>

        <ssl enabled="true">
            <factory-class-name>
                com.hazelcast.nio.ssl.BasicSSLContextFactory
            </factory-class-name>
            <properties>
                <property name="javax.net.ssl.keyStore">hazelcast.keystore</property>
                <property name="javax.net.ssl.keyStorePassword">123456</property>
                <property name="javax.net.ssl.trustStore">hazelcast.truststore</property>
                <property name="javax.net.ssl.trustStorePassword">123456</property>
                <property name="keyManagerAlgorithm">SunX509</property>
                <property name="trustManagerAlgorithm">SunX509</property>
                <property name="protocol">TLS</property>
            </properties>
        </ssl>

    </network>

    <serialization>
        <portable-version>1</portable-version>
        <portable-factories>
            <portable-factory factory-id="10000000">com.hazelcast.stabilizer.tests.synthetic.SyntheticRequestPortableFactory</portable-factory>
        </portable-factories>
    </serialization>

</hazelcast-client>